{
    "name": "octopus_unit",
    "version": "0.0.0",
    "description": "a versatile unit testing utility tool",
    "scripts": {
        "test": "rm -rf .nyc_output && electron-mocha --require ./babel-register.testing.js src/**/*.spec.ts",
        "cover": "npm run test && nyc --clean=false --require ./babel-register.testing.js --instrument=false --source-map=false --all=true -- node -e ''",
        "build:dev": "webpack --config webpack/development.config.js --watch",
        "start:dev": "npm run build:dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YiZhang-Paul/Octopus_Unit.git"
    },
    "keywords": [
        "unit",
        "testing"
    ],
    "author": "Yi Zhang",
    "bugs": {
        "url": "https://github.com/YiZhang-Paul/Octopus_Unit/issues"
    },
    "homepage": "https://github.com/YiZhang-Paul/Octopus_Unit#readme",
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-decorators": "^7.7.4",
        "@babel/preset-env": "^7.7.7",
        "@babel/preset-typescript": "^7.7.7",
        "@babel/register": "^7.7.7",
        "@types/chai": "^4.2.7",
        "@types/electron": "^1.6.10",
        "@types/fs-extra": "^8.0.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^13.1.1",
        "@types/sinon": "^7.5.1",
        "@vue/test-utils": "^1.0.0-beta.30",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-plugin-parameter-decorator": "^1.0.14",
        "chai": "^4.2.0",
        "css-loader": "^3.4.0",
        "electron": "^7.1.7",
        "electron-mocha": "^8.1.2",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.9.0",
        "mocha": "^6.2.2",
        "node-sass": "^4.13.0",
        "nyc": "^15.0.0",
        "sass-loader": "^8.0.0",
        "sinon": "^8.0.2",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.4",
        "vue-loader": "^15.8.3",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.10",
        "webpack-electron-reload": "^1.0.0",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "inversify": "^5.0.1",
        "reflect-metadata": "^0.1.13",
        "vue": "^2.6.11",
        "vuex": "^3.1.2"
    }
}
