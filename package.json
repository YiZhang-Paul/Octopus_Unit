{
    "name": "octopus_unit",
    "version": "0.0.0",
    "description": "a versatile unit testing utility tool",
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "instrument": false,
        "sourceMap": false
    },
    "scripts": {
        "test": "mochapack --webpack-config webpack/testing.config.js \"src/**/*.spec.ts\"",
        "cover": "nyc --reporter=lcov --reporter=text npm run test",
        "build:dev": "webpack --config webpack/development.config.js --watch",
        "start:dev": "npm run build:dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YiZhang-Paul/Octopus_Unit.git"
    },
    "keywords": [
        "unit",
        "testing"
    ],
    "author": "Yi Zhang",
    "bugs": {
        "url": "https://github.com/YiZhang-Paul/Octopus_Unit/issues"
    },
    "homepage": "https://github.com/YiZhang-Paul/Octopus_Unit#readme",
    "devDependencies": {
        "@types/chai": "^4.2.7",
        "@types/fs-extra": "^8.0.1",
        "@types/highlight.js": "^9.12.3",
        "@types/mocha": "^5.2.7",
        "@types/node": "^13.1.1",
        "@types/sinon": "^7.5.1",
        "@vue/test-utils": "^1.0.0-beta.30",
        "chai": "^4.2.0",
        "css-loader": "^3.4.0",
        "electron": "^18.3.7",
        "html-webpack-plugin": "^3.2.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "mini-css-extract-plugin": "^0.9.0",
        "mocha": "^6.2.2",
        "mochapack": "^1.1.13",
        "node-sass": "^4.13.0",
        "nyc": "^15.0.0",
        "sass-loader": "^8.0.0",
        "sinon": "^8.0.2",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.4",
        "vue-loader": "^15.8.3",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.10",
        "webpack-electron-reload": "^1.0.0",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "highlight.js": "^9.17.1",
        "inversify": "^5.0.1",
        "reflect-metadata": "^0.1.13",
        "vue": "^2.6.11",
        "vuex": "^3.1.2"
    }
}
